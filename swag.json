                    "200": {
                        "description": "Successfully fetched the data"
                    }
                }
            }
        },
        "/app/update/": {
            "put": {
                "description": "Api of insert",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "student object the need to be update",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successfully Inserted."
                    }
                }
            }
        },
        "/app/delete/": {
            "delete": {
                "description": "Api of insert",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "student object the need to be update",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successfully Inserted."
                    }
                }
            }
